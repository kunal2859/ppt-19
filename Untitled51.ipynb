   "source": [
    "def calculate_mean(numbers):\n",
    "    total=sum(numbers)\n",
    "    count=len(numbers)\n",
    "    mean=total/count\n",
    "    return mean\n",
    "\n",
    "n=int(input())\n",
    "num=list(map(int,input().split()))[:n]\n",
    "Mean=calculate_mean(num)\n",
    "print(\"Mean: \", Mean)"
   ]
